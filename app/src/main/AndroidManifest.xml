<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jakeg.groupupapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activity.SettingsActivity"></activity>
        <activity android:name=".activity.activity.ContactsActivity" />
        <activity android:name=".activity.activity.UserProfileActivity" />
        <activity android:name=".activity.activity.GroupMemberActivity" />
        <activity android:name=".activity.activity.GroupActivity" />
        <activity android:name=".activity.activity.SetGroupMeetingTimeActivity" />
        <activity android:name=".activity.activity.GroupListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.activity.NameGroupActivity" />
        <activity android:name=".activity.activity.AddContactsToGroupActivity" />
    </application>

</manifest>